{
    "root": true,
    "extends": [
        "plugin:prettier/recommended",
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
    },
    "ignorePatterns": [
        "**/build/*",
        "**/node_modules/*"
    ],
    "rules": {
        "no-use-before-define": "off",
    }
}
